  "scripts": {
    "build:tailwind": "tailwindcss build src/App.css -o src/index.css",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind",
    "watch:dev": "nodemon -x npm run build:tailwind -w tailwind.config.js -w src/App.css",
    "watch:css": "chokidar 'src/**/*.css' --ignore src/index.css -c 'npm run build:tailwind'",
    "start": "react-scripts start",
    "watch": "run-p build:tailwind watch:css watch:dev start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
